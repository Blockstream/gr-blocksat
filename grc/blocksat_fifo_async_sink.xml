<?xml version="1.0"?>
<block>
  <name>Asynchronous FIFO Sink</name>
  <key>blocksat_fifo_async_sink</key>
  <category>[Blockstream Satellite]/FIFO</category>
  <import>import blocksat</import>
  <make>blocksat.fifo_async_sink($fifo_file)</make>
  <param>
    <name>Fifo File</name>
    <key>fifo_file</key>
    <type>string</type>
  </param>
  <sink>
    <name>async_pdu</name>
    <type>message</type>
  </sink>
</block>
