<?xml version="1.0"?>
<block>
  <name>Non-blocking File Sink</name>
  <key>mods_file_sink_nonblock</key>
  <category>[Blockstream]/File Operators</category>
  <import>import mods</import>
  <make>mods.file_sink_nonblock($file_name)</make>
  <param>
    <name>Pipe Name</name>
    <key>file_name</key>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
</block>
